    "prettier": "2.0.5",
    "tailwindcss": "1.3.5",
    "twitter": "^1.7.1",
    "typescript": "3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aravindballa/learningcurve.dev"
  },
  "bugs": {
    "url": "https://github.com/aravindballa/learningcurve.dev/issues"
