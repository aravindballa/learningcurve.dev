    "gatsby-plugin-offline": "3.1.4",
    "gatsby-plugin-postcss": "2.2.3",
    "gatsby-plugin-react-helmet": "3.2.4",
    "gatsby-plugin-tinacms": "^0.1.35",
    "gatsby-plugin-transitions": "^2.1.6",
    "gatsby-source-anchor": "^0.5.1",
    "gatsby-source-filesystem": "^2.2.4",
    "graphql": "15.0.0",
    "pretty-ms": "^6.0.1",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
